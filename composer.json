{
    "name": "hutchh/ip-geoinfo",
    "description": "IP Geo address info",
    "type": "library",
    "license": "mit",
    "version": "1.0.0",
    "autoload": {
        "psr-4": {
            "Hutchh\\IpGeoinfo\\": "src/"
        },
        "classmap": ["src/"]
    },
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Hutchh",
            "email": "satnam.hutchh@gmail.com",
            "role": "Developer"
        }
    ],
    "suggest": {
        "monolog/monolog": "Allows more advanced logging of the application flow",
        "ext-xml": "Needed to support XML format in class Foo"
    },
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.2",
        "illuminate/http": "^8.0 || ^9.0 || ^10.0 || ^11.0",
        "illuminate/support": "^8.0 || ^9.0 || ^10.0 || ^11.0",
        "illuminate/contracts": "^8.0 || ^9.0 || ^10.0 || ^11.0",
        "psr/http-message": "^1.0 || ^2.0",
        "nesbot/carbon": "^2.67"
    },
    "homepage":"https://www.hutchh.com.au",
    "support": {
        "email": "support@example.org",
        "irc": "irc://irc.freenode.org/composer"
    },
    "funding": [
        {
            "type": "patreon",
            "url": "https://www.patreon.com/phpdoctrine"
        },
        {
            "type": "tidelift",
            "url": "https://tidelift.com/subscription/pkg/packagist-doctrine_doctrine-bundle"
        },
        {
            "type": "other",
            "url": "https://www.doctrine-project.org/sponsorship.html"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Hutchh\\IpGeoinfo\\Providers\\IpGeoinfoServiceProvider"
            ],
            "aliases": {
                "Debugbar": "Hutchh\\IpGeoinfo\\Facades\\IpGeoinfoFacade"
            }
        }
    }
}
